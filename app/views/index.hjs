<!DOCTYPE html>
<html>
  <head>
    <title>{{ title }}</title>
	<link rel="stylesheet/less" type="text/css" href="styles/styles.less" />
	<link href='http://fonts.googleapis.com/css?family=Roboto+Slab' rel='stylesheet' type='text/css'>
  </head>
  <body>

	<ul id="scene">
		<!--<li class="layer" data-depth="0.00"><h1>Layer 1</h1></li>-->
		<li class="layer" data-depth="0.10"><div class="background"></div></li>
		<!--
		<li class="layer" data-depth="0.10">
			<h1>{{ title }}</h1>
			<p>Welcome to  {{ title }}</p>
			<form method="post" action="/">
				<input type="submit" value="open-ie"/>
			</form>
		</li>
		<li class="layer" data-depth="0.20"><h1>Layer 2</h1></li>
		<li class="layer" data-depth="0.40"><h1>Layer 3</h1></li>
		<li class="layer" data-depth="0.60"><h1>Layer 4</h1></li>
		<li class="layer" data-depth="0.80"><h1>Layer 5</h1></li>-->
		<li class="layer" data-depth="0">
			<div id="Chrome" class="icon chrome launch" data-launch-type="launchExe" data-launch-location="C:/Program Files (x86)/Google/Chrome/Application/chrome.exe" data-activation-title="Chrome">
				<p class="caption">Chrome</p>
			</div>
			<div id="notepad" class="icon notepad launch" data-launch-type="launchExe" data-launch-location="C:/Program Files (x86)/Notepad++/notepad++.exe" data-activation-title="Notepad">
				<p class="caption">Notepad ++</p>
			</div>
			<div id="spotify" class="icon spotify launch" data-launch-type="launchExe" data-launch-location="C:/Users/Administrator/AppData/Roaming/Spotify/spotify.exe" data-activation-title="Spotify">
				<p class="caption">Spotify</p>
			</div>
			<div class="icon xbmc launch" data-launch-type="launchExe" data-launch-location="C:/Program Files (x86)/XBMC/XBMC.exe" data-activation-title="XBMC">
				<p class="caption">XBMC</p>
			</div>
			<div class="icon gamecube launch" data-launch-type="launchCommand" data-command='dolphin /b /e "C:\tmp\Dragon Ball Z - Budokai 2.iso"' data-activation-title="Dolphin">
				<p class="caption">GameCube</p>
			</div>
		</li>
	</ul>
	
    <script>document.write('<script src="http://' + (location.host || 'localhost').split(':')[0] + ':35729/livereload.js?snipver=1"></' + 'script>')</script>
	<script src="js/libs/less-1.7.3.min.js"></script>
	<script src="js/libs/jquery-2.1.1.min.js"></script>
	<script src="js/libs/jquery.parallax.js"></script>
	<script src="js/bootstrap.js"></script>
  </body>
</html>